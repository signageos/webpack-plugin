{
	"name": "@signageos/webpack-plugin",
	"version": "1.0.9",
	"description": "Webpack Plugin for emulating sOS JS API in browser",
	"main": "dist/index.js",
	"files": [
		".env",
		"dist",
		"README.md",
		"package.json",
		"package-lock.json",
		"config"
	],
	"scripts": {
		"clean": "rm -rf dist/*",
		"clean-build": "npm run clean && npm run build",
		"clean-build-public": "npm run clean-build",
		"build": "tsc",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"lint:prettier": "prettier  \"**/*.+(ts|tsx|json|js)\" --check",
		"lint:prettier:fix": "prettier  \"**/*.+(ts|tsx|json|js)\" --write",
		"test": "mocha",
		"watch": "tsc --watch",
		"check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib-ci check-deps",
		"check-types": "tsc --noEmit",
		"depcheck": "depcheck --config .depcheckrc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/signageos/webpack-plugin.git"
	},
	"keywords": [
		"webpack",
		"signageOS",
		"plugin"
	],
	"author": "signageOS <dev@signageos.io>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/signageos/webpack-plugin/issues"
	},
	"homepage": "https://github.com/signageos/webpack-plugin#readme",
	"dependencies": {
		"@signageos/cli": "^2.6.0-mikulas-audit-clean.2043",
		"@signageos/sdk": "2.0.2",
		"chalk": "4.1.2",
		"command-line-args": "6.0.1",
		"cors": "2.8.5",
		"debug": "4.4.1",
		"express": "4.21.2",
		"fs-extra": "11.3.0",
		"mime": "4.0.7",
		"serve-static": "2.2.0"
	},
	"overrides": {
		"underscore": "^1.13.6",
		"mongoose": "^8.9.5",
		"socket.io-parser": "^4.2.4",
		"debug": "4.4.1",
		"ms": "^2.1.3",
		"qs": "^6.13.0",
		"connect": "^3.7.0",
		"parsejson": "0.0.3",
		"parseuri": "3.0.2",
		"socket.io-client": "^4.8.1"
	},
	"peerDependencies": {
		"@signageos/front-display": ">=14.0.0",
		"webpack": ">=4.0.0"
	},
	"devDependencies": {
		"@signageos/codestyle": "2.1.0",
		"@signageos/front-display": "^14.22.0",
		"@types/command-line-args": "5.2.3",
		"@types/command-line-usage": "5.0.4",
		"@types/cors": "2.8.17",
		"@types/debug": "4.1.12",
		"@types/express": "4.17.21",
		"@types/fs-extra": "11.0.4",
		"@types/ini": "4.1.1",
		"@types/mime": "3.0.4",
		"@types/mocha": "10.0.10",
		"@types/node": "22.15.2",
		"@types/node-fetch": "2.6.12",
		"@types/serve-static": "1.15.7",
		"depcheck": "1.4.7",
		"mocha": "11.1.0",
		"ts-node": "10.9.2",
		"typescript": "5.8.3",
		"webpack": "5.99.7"
	}
}
