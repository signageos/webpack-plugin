{
  "name": "@signageos/webpack-plugin",
  "version": "1.0.8",
  "description": "Webpack Plugin for emulating sOS JS API in browser",
  "main": "dist/index.js",
  "files": [
    ".env",
    "dist",
    "README.md",
    "package.json",
    "package-lock.json",
    "config"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src/ ./tests/",
    "lint:fix": "eslint --fix ./src/ ./tests/",
    "lint:prettier": "prettier  \"**/*.+(ts|tsx|json|js)\" --check",
    "lint:prettier:fix": "prettier  \"**/*.+(ts|tsx|json|js)\" --write",
    "clean": "rm -rf dist/*",
    "clean-build": "npm run clean && npm run build",
    "test": "mocha",
    "watch": "tsc --watch",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib-ci check-deps",
    "depcheck": "depcheck --specials=tslint,webpack,mocha --parsers='**/*.ts:typescript,**/*.js:es6' --detectors='requireCallExpression,importDeclaration' --ignore-dirs='dist' --ignores='@types/*,@signageos/codestyle,mocha,depcheck,webpack,@signageos/front-display'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signageos/webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "signageOS",
    "plugin"
  ],
  "author": "signageOS.io <dev@signageos.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signageos/webpack-plugin/issues"
  },
  "homepage": "https://github.com/signageos/webpack-plugin#readme",
  "dependencies": {
    "@signageos/cli": "^2.3.0",
    "@signageos/sdk": "^1.27.3",
    "chalk": "4.1.2",
    "command-line-args": "6.0.1",
    "cors": "2.8.5",
    "debug": "4.4.0",
    "express": "5.1.0",
    "fs-extra": "11.3.0",
    "mime": "2.5.2",
    "serve-static": "2.2.0"
  },
  "peerDependencies": {
    "@signageos/front-display": ">=14.0.0",
    "webpack": ">=4.0.0"
  },
  "devDependencies": {
    "@signageos/codestyle": "0.4.0",
    "@signageos/front-display": "^14.18.4",
    "@types/command-line-args": "5.2.3",
    "@types/command-line-usage": "5.0.4",
    "@types/cors": "2.8.17",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/ini": "4.1.1",
    "@types/mime": "3.0.4",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.2",
    "@types/node-fetch": "2.6.12",
    "@types/serve-static": "1.15.7",
    "depcheck": "1.4.7",
    "mocha": "11.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.0",
    "webpack": "5.99.7"
  }
}
